allow vendor_init proc:lnk_file { setattr };

# Allow vendor_init to relabel unlabeled files and directories
allow vendor_init unlabeled:{ dir file } { getattr relabelfrom };

# A12
set_prop(vendor_init, camera_prop);
set_prop(vendor_init, vendor_power_prop);
set_prop(vendor_init, vendor_qcom_ims_prop);
set_prop(vendor_init, vendor_time_service_prop);

set_prop(vendor_init, persist_debug_prop);
set_prop(vendor_init, bluetooth_prop);
set_prop(vendor_init, default_prop);
set_prop(vendor_init, thermal_normal_prop);
set_prop(vendor_init, vendor_tee_listener_prop);

allow vendor_init vendor_tee_listener_prop:file { open read };
allow vendor_init proc:file w_file_perms;
